{
  "name": "astha-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:ui": "vite",
    "dev:api": "npm --prefix ../astha-chatbot-app/server run dev",
    "dev:bank": "npm --prefix ../mock-bank-api run start",
    "dev:server": "ts-node --transpile-only server/index.ts",
    "dev:all": "concurrently -k -n UI,API,SERVER -c blue,magenta,yellow \"npm run dev\" \"npm run dev:api\" \"npm run dev:server\"",
    "dev:full": "concurrently -k -n UI,CHAT,BANK -c blue,magenta,green \"npm run dev\" \"npm run dev:api\" \"npm run dev:bank\""
  },
  "dependencies": {
    "@vapi-ai/web": "^2.3.10",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.1",
    "react-country-flag": "^3.1.0",
    "react-dom": "^19.1.1",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.1.2",
    "retell-client-js-sdk": "^2.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.4.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
